{
    "name": "Bitcamp 2024",
    "version": "1.0.0",
    "description": "",
    "manifest_version": 3,
    "author": "Declan Scott, Ryan Shechtman, & Tanay Naik",
    "action":{
        "default_popup": "index.html",
        "default_title": "Bitcamp 2024"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["injection/tabinject.js"],
          "css": ["injection/injectedstyle.css"]
        }
    ]
}